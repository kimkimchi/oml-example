@dc:title "Propulsion Assemblies"
description <http://example.io/programs/earth-science/projects/firesat/workpackages/06/08/subsystems/prp/assemblies> with # as prp-assemblies {

	extends <http://example.io/programs/earth-science/projects/firesat/workpackages/06/systems/spc/subsystems/prp/prp>

	// Propulsion Subsystem
	
	ref ci prp:Propulsion [
		base:aggregates PropulsionSystemControlUnit
		base:aggregates PrimaryThruster
		base:aggregates ACSThruster1
		base:aggregates ACSThruster2
		base:aggregates ACSThruster3
		base:aggregates ACSThruster4
		base:aggregates ACSThruster5
		base:aggregates ACSThruster6
		base:aggregates PropellantHandlingSection
		base:aggregates FillDrainValveUnit
		base:aggregates BPPropellantLineUnit
		base:aggregates PropulsionModule
		base:aggregates PropellantTank
		base:aggregates PressurantTank
		base:aggregates PMPropellantLineUnit
	]
	
	// Propulsion System Control Unit

 	@rdfs:label "Propulsion System Control Unit"
	ci PropulsionSystemControlUnit : system:Assembly [
		base:hasIdentifier "516700"
	]

	// Primary Thruster

	@rdfs:label "Primary Thruster"
	ci PrimaryThruster : system:Assembly [
		base:hasIdentifier "517000"
	]

	// ACS Thruster 1

	@rdfs:label "ACS Thruster 1"
	ci ACSThruster1 : system:Assembly [
		base:hasIdentifier "518000-1"
	]

	// ACS Thruster 2

	@rdfs:label "ACS Thruster 2"
	ci ACSThruster2 : system:Assembly [
		base:hasIdentifier "518000-2"
	]

	// ACS Thruster 3

	@rdfs:label "ACS Thruster 3"
	ci ACSThruster3 : system:Assembly [
		base:hasIdentifier "518000-3"
	]

	// ACS Thruster 4

	@rdfs:label "ACS Thruster 4"
	ci ACSThruster4 : system:Assembly [
		base:hasIdentifier "518000-4"
	]

	// ACS Thruster 5

	@rdfs:label "ACS Thruster 5"
	ci ACSThruster5 : system:Assembly [
		base:hasIdentifier "518000-5"
	]

	// ACS Thruster 6

	@rdfs:label "ACS Thruster 6"
	ci ACSThruster6 : system:Assembly [
		base:hasIdentifier "518000-6"
	]

	// Propellant Handling Section

	@rdfs:label "Propellant Handling Section"
	ci PropellantHandlingSection : system:Assembly [
		base:hasIdentifier "519000"
	]

	// FillDrain Valve Unit

	@rdfs:label "Fill/Drain Valve Unit"
	ci FillDrainValveUnit : system:Assembly [
