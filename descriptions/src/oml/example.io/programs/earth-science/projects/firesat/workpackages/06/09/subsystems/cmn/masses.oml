@dc:title "Communications Masses"
description <http://example.io/programs/earth-science/projects/firesat/workpackages/06/09/subsystems/cmn/masses> with # as cmn-masses {

	extends <http://example.io/programs/earth-science/projects/firesat/workpackages/06/09/subsystems/cmn/assemblies>

	// Transmitter Unit Mass

	@rdfs:label "Transmitter Unit CBE Mass Constraint"
	ci TransmitterUnitMassAllocationConstraint : system:MassAllocationConstraint [
		metrology:hasDoubleNumber "2.1"^^xsd:double
    metrology:hasReference 80000-4-instances:kilogram
		metrology:isQuantityOf cmn-assemblies:TransmitterUnit
	]

	// Receiver Unit Mass

	@rdfs:label "Receiver Unit CBE Mass Constraint"
	ci ReceiverUnitMassAllocationConstraint : system:MassAllocationConstraint [
		metrology:hasDoubleNumber "1.0"^^xsd:double
    metrology:hasReference 80000-4-instances:kilogram
		metrology:isQuantityOf cmn-assemblies:ReceiverUnit
	]

	// Transmit Whip Antenna Mass

	@rdfs:label "Transmit Whip Antenna CBE Mass Constraint"
	ci TransmitWhipAntennaMassAllocationConstraint : system:MassAllocationConstraint [
		metrology:hasDoubleNumber "0.04"^^xsd:double
    metrology:hasReference 80000-4-instances:kilogram
		metrology:isQuantityOf cmn-assemblies:TransmitWhipAntenna
	]

	// Transmit Hi-Gain Antenna Mass

	@rdfs:label "Transmit Hi-Gain Antenna CBE Mass Constraint"
	ci TransmitHiGainAntennaMassAllocationConstraint : system:MassAllocationConstraint [
		metrology:hasDoubleNumber "0.04"^^xsd:double
    metrology:hasReference 80000-4-instances:kilogram
		metrology:isQuantityOf cmn-assemblies:TransmitHiGainAntenna
	]

	// Receive Whip Antenna Mass

	@rdfs:label "Receive Whip Antenna CBE Mass Constraint"
	ci ReceiveWhipAntennaMassAllocationConstraint : system:MassAllocationConstraint [
		metrology:hasDoubleNumber "0.08"^^xsd:double
    metrology:hasReference 80000-4-instances:kilogram
		metrology:isQuantityOf cmn-assemblies:ReceiveWhipAntenna
	]

}